class Solution {
    public int removeElement(int[] nums, int val) {
        int place_index = 0;
        for(int i = 0; i < nums.length; i++){
            int number = nums[i];
            if(number != val){
                nums[place_index] = number;
                place_index++;
            }
        }
        
        return place_index;
    }
}
